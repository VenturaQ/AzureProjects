{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-19-report-2023"
		},
		"ls_ablob_covidstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidstorage'"
		},
		"ls_adls_covidstoragedatalake2023_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidstoragedatalake2023'"
		},
		"ls_adls_covidstoragedatalake2023_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidstoragedatalake2023.dfs.core.windows.net/"
		},
		"ls_http_opendata_ecdc_europa_eu_fixed_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@linkedService().sourceBaseURL"
		},
		"ls_http_opendata_ecde_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"tr_ingest_covid_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/c035afc9-633a-4e61-b6f8-d8acce1ef253/resourceGroups/Covid-19/providers/Microsoft.Storage/storageAccounts/covid19storage2023"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-19-report-2023_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidstorage_connectionString": {
						"value": "[parameters('ls_ablob_covidstorage_connectionString')]"
					},
					"ls_adls_covidstoragedatalake2023_accountKey": {
						"value": "[parameters('ls_adls_covidstoragedatalake2023_accountKey')]"
					},
					"ls_adls_covidstoragedatalake2023_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidstoragedatalake2023_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_europa_eu_fixed_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_europa_eu_fixed_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecde_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecde_europa_eu_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "covid-19-report-2023_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_ingest_covid_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_covid_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covid-19-report-2023_ArmTemplate_0')]"
			]
		}
	]
}